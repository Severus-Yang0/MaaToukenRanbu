{
    "version": "1.0.0",
    "message": "刀剑乱舞自动化助手 - 支持刷花、演练、练级功能",
    "controller": [
        {
            "name": "安卓端",
            "type": "Adb"
        }
    ],
    "resource": [
        {
            "name": "官服",
            "path": [
                "{PROJECT_DIR}/resource"
            ]
        }
    ],
    "agent": {
        "child_exec": "python",
        "child_args": [
            "{PROJECT_DIR}/agent/main.py"
        ]
    },
    "task": [
        {
            "name": "一键刷花",
            "entry": "刷花_任务入口"
        },
        {
            "name": "一键演练",
            "entry": "演练_任务入口"
        },
        {
            "name": "一键5-4练级（不进入王点）",
            "entry": "练级54_任务入口"
        },
        {
            "name": "一键战扩关卡1",
            "entry": "战扩关卡1_任务入口"
        }
    ]
}